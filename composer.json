{
    "name": "lsv-eu/livewire-calendar",
    "description": "Laravel Livewire calendar component with Livewire v3",
    "keywords": [
        "asantibanez",
        "lsv-eu",
        "little-sane-village",
        "livewire-calendar"
    ],
    "homepage": "https://github.com/lsv-eu/livewire-calendar",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Leo Lutz",
            "email": "leo.lutz@lsv.eu",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "livewire/livewire": "^v3.0.0-beta.7"
    },
    "require-dev": {
        "laravel/pint": "^1.10",
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "LittleSaneVillage\\LivewireCalendar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LittleSaneVillage\\LivewireCalendar\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LittleSaneVillage\\LivewireCalendar\\LivewireCalendarServiceProvider"
            ]
        }
    }
}
